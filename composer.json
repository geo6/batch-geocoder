{
    "name": "geo6/batch-geocoder",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jonathan BeliÃ«n",
            "email": "jbe@geo6.be"
        }
    ],
    "require": {
        "php": "^7.2",
        "geo6/geocoder-php-bpost-provider": "^1.1",
        "geo6/geocoder-php-geopunt-provider": "^1.0",
        "geo6/geocoder-php-spw-provider": "^1.0",
        "geo6/geocoder-php-urbis-provider": "^1.0",
        "geo6/php-text-tools": "^1.0",
        "geocoder-php/chain-provider": "^4.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "zendframework/zend-component-installer": "^2.0",
        "zendframework/zend-config": "^3.1",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-diactoros": "^1.7",
        "zendframework/zend-expressive": "^2.1",
        "zendframework/zend-expressive-fastroute": "^2.1",
        "zendframework/zend-expressive-flash": "^0.1.0",
        "zendframework/zend-expressive-helpers": "^4.2",
        "zendframework/zend-expressive-platesrenderer": "^1.4",
        "zendframework/zend-expressive-session-ext": "^0.1.2",
        "zendframework/zend-filter": "^2.7",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-validator": "^2.10"
    },
    "require-dev": {
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@init:directory",
            "@init:sql"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ -d display_errors=1",
        "gettext": [
            "@gettext:fr_BE:po",
            "@gettext:nl_BE:po"
        ],
        "gettext:mo": [
            "@gettext:fr_BE:mo",
            "@gettext:nl_BE:mo"
        ],
        "gettext:fr_BE:po": "mkdir -p locale/fr_BE && find templates/ -iname \"*.phtml\" | xargs xgettext --from-code=utf-8 --no-location --sort-by-file --join-existing --keyword=translate --keyword=translatePlural:1,2 --language=PHP --output-dir=locale/fr_BE",
        "gettext:fr_BE:mo": "msgfmt -c -o locale/fr_BE/messages.mo locale/fr_BE/messages.po",
        "gettext:nl_BE:po": "mkdir -p locale/nl_BE && find templates/ -iname \"*.phtml\" | xargs xgettext --from-code=utf-8 --no-location --sort-by-file --join-existing --keyword=translate --keyword=translatePlural:1,2 --language=PHP --output-dir=locale/nl_BE",
        "gettext:nl_BE:mo": "msgfmt -c -o locale/nl_BE/messages.mo locale/nl_BE/messages.po",
        "init:directory": "mkdir -p data/cache && mkdir -p data/upload && sudo chmod -R 0777 data/",
        "init:sql": "sudo -u postgres createuser --pwprompt geocode && sudo -u postgres psql -f scripts/create-database.sql && sudo -u postgres psql -f scripts/create-extension.sql geocode"
    }
}
