{
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.0-11",
    "@fortawesome/free-brands-svg-icons": "5.1.0-8",
    "@fortawesome/free-regular-svg-icons": "5.1.0",
    "@fortawesome/free-solid-svg-icons": "5.1.0-8",
    "ol": "4.6.5"
  },
  "devDependencies": {
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "node-sass": "4.9.0",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8"
  },
  "scripts": {
    "build": "npm run build:css & npm run build:js",
    "build:css": "node-sass --output-style compressed --output public/css resources/sass/style.scss",
    "build:js": "webpack --config webpack.config.js",
    "test": "eslint resources/javascript/",
    "fix": "eslint resources/javascript/ --fix",
    "watch": "npm run watch:css",
    "watch:css": "node-sass --watch --output-style compressed --output public/css resources/sass/style.scss"
  }
}
